
@{
    Layout = null;
}

@using EstudyCorner.Models
@model IEnumerable<notification>
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Welcome to E-study Vidya </title>

    <link href="~/Content/bootstrap.css" rel="stylesheet" />


    <script src="~/Scripts/jquery-3.3.1.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>

    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css">

    <!-- jQuery library -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

    <!-- Popper JS -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>

    <!-- Latest compiled JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js"></script>
    <style>
        #social 
        {
            list-style-type:none;
            margin-left:15px;
        }
        #social img
        {
            width:40px;
            height:40px;
            margin:5px;
        }
        #social
        {
            color:white;
            font-size:18px;
            font-weight:600;
        }
  

    </style>

</head>
<body>
    <div class="container-fluid">
        <div class="row" style="background-color:black;min-height:50px;padding-top:10px">
            <div class="col-sm-3 text-center">
                <span style="color:white;font-size:17px;font-weight:800;">Have any Question ?</span>
            </div>
            <div class="col-sm-3 text-center">
                <span style="color:white;font-size:17px;font-weight:800;" class="glyphicon glyphicon-phone-alt"> Call us- 9369474565 </span>
            </div>
            <div class="col-lg-6 text-right">
                <a href="Register" style="color:white;font-size:18px;font-weight:800;"><span class="glyphicon glyphicon-log-in"></span> Register / SignUp</a>
                <!-- @Html.ActionLink("Register / SignUp", "Register", null, new { style = "color:white;font-size:18px;font-weight:800;" })-->
            </div>

        </div>
        <div class="row" style="background-color:white;min-height:70px;padding:0px;">
            <div class="col-sm-2" style="padding-left:30px;">
                <!--logo-->
                <a href="Index"><img src="~/Content/Images/mylogo.png" height="110px" width="110px" /></a>
            </div>
            <div class="col-sm-10 text-center" style="padding:0px;margin-bottom:0px">

                <nav class="navbar navbar-expand-lg navbar-light text-center " style="margin:15px 0px">
                    <!-- <a class="navbar-brand" href="#">Navbar</a>-->
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarNav">
                        <ul class="navbar-nav" style="font-size:25px;">
                            <li class="nav-item active">
                                <a href="Index" class="nav-link" style="font-weight:700;padding-left:20px;color:black;"> <span class=" glyphicon glyphicon-home"></span> Home</a>
                                <!--@Html.ActionLink("Home", "/", null, new { @class = "nav-link", style = "font-weight:700;padding-left:20px;color:black;" })-->

                            </li>
                            <li class="nav-item">
                                <a href="/Home/VisionMission" class="nav-link" , style="font-weight:700;padding-left:20px;color:black;"> <span class="glyphicon glyphicon-eye-open"></span> Vision & Mission</a>
                                <!-- @Html.ActionLink("Vision & Mission", "VisionMission", null, new { @class = "nav-link", style = "font-weight:700;padding-left:20px;color:black;" })-->

                            </li>
                            <li class="nav-item">
                                <a href="Contact" class="nav-link" , style="font-weight:700;padding-left:20px;color:black;"><span class=" glyphicon glyphicon-phone"></span> Contact Us</a>
                                <!-- @Html.ActionLink("Contact Us", "Contact", null, new { @class = "nav-link", style = "font-weight:700;padding-left:20px;color:black;" })-->

                            </li>
                            <li class="nav-item">
                                <a href="Login" class="nav-link" , style="font-weight:700;padding-left:20px;color:black;"><span class=" glyphicon glyphicon-log-in"></span> Login</a>
                                <!-- @Html.ActionLink("Login", "Login", null, new { @class = "nav-link", style = "font-weight:700;padding-left:20px;color:black;" })-->

                            </li>
                        </ul>
                    </div>
                </nav>
            </div>

        </div>

        <div class="row" style="background-color:black;min-height:30px;">
            <marquee onmouseover="stop()" onmouseout="start()">

                @{
                    int x = 1;
                    foreach (notification n in Model)
                    {
                        <span style="color:white;font-size:20px;"><img src="~/Content/icon/bullet.gif" style="width:18px;border-radius:50%;" /> @n.msg</span>
                        if (x == 1)
                        {
                            <img src="~/Content/icon/new.gif" width="55px" height="35px" />
                        }
                        x++;

                    }
                }
            </marquee>
        </div>
        <div class="row" style="margin:0px;padding:0px;">
            @RenderBody()
        </div>
        <div class="row" style="background-color:black;min-height:250px">
            <!--foooter-->
            <div class="col-sm-4" style="padding-left:30px;">
                <h3 style="color:white;padding-top:5px;"> Quick links</h3>
                <hr style="color:white;">
                <p style="font-size:14px;font-weight:700;color:white;">For Reference more</p><br />
                <br />
                @Html.ActionLink("Swayam Official", "https://www.swayam.gov.in", null, new { style = "font-size:20px; color: white;font-weight:600; " })<br /><br />
                @Html.ActionLink("Learn | code ", "https://code.org", null, new { style = "font-size:20px; color: white;font-weight:600;" })<br /><br />
                @Html.ActionLink("Virtual lab", "https://www.vlab.co.in", null, new { style = "font-size:20px; color: white;font-weight:600;" })<br /><br />

            </div>
            <div class="col-sm-4">
                <p style="color:white;font-size:18px;padding-top:5px;">Social works</p>
                <hr style="color:white;">
                <ul id="social">
                    <li><a href="https://www.linkedin.com/" style="color:white;"> <img src="~/Content/Images/linkedin.png"  />LinkdIn</a></li>
                    <li> <a href="https://www.facebook.com/" style="color:white;"> <img src="~/Content/Images/fb.png" />Facebook</a></li>
                    <li> <a href="https://www.twitter.com/" style="color:white;"><img src="~/Content/Images/twitter.png" />Twitter</a></li>
                    <li> <a href="https://www.youtube.com/" style="color:white;"><img src="~/Content/Images/youtube.png" />Youtube</a></li>
                </ul>

            </div>


            <div class="col-sm-4">
                <h3 style="color:white;padding-top:5px;"> Address</h3>
                <hr style="color:white;">
                <p style="font-size:20px;padding-top:10px;color:white;font-weight:500">
                    Governmet polytechnic jaunpur.<br />
                    Jagdishpur,jaunpur-220132
                    <br /><br />
                    mobile no:- 9369474565

                </p>
            </div>


        </div>
        <div class="row text-center" style="background-color:black;padding-bottom:10px;">
            <div class="col-sm-6 text-center" style="color:white;font-size:15px;font-weight:600;">This Content is &copy; copyright by E-study Vidya </div>
            <div class="col-sm-6 text-center" style="color:white;font-size:15px;font-weight:600;">Design & Developed By-<a href="Personal" style="color:white;">Alok Jaiswal</a> </div>
        </div>

    </div>
</body>
</html>
